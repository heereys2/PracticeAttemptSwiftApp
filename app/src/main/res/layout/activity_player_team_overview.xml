<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PlayerTeamOverview"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Team"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="27dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upcoming Events"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="77dp" />

    <TextView
        android:id="@+id/tv_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="120dp" />

    <Button
        android:id="@+id/btnRecTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onRecordTime"
        android:text="Record Time"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="201dp" />

    <Button
        android:id="@+id/btnEnterData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onEnterData"
        android:text="Enter Data"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="201dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onViewResults"
        android:text="View Results"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="518dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View results for"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="389dp" />

    <Spinner
        android:id="@+id/spinner_dataEntryResults"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="430dp" />

</android.support.constraint.ConstraintLayout>